{
  "name": "express-server-experiment",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "main": "server.ts",
  "scripts": {
    "dev": "NODE_ENV=development dotenv -e .env.development -- nodemon",
    "build": "tsc && npm-run-all build:*",
    "build:server": "vite build --ssr src/server/index.ts --outDir dist/server",
    "build:client": "vite build --outDir dist/client --ssrManifest",
    "lint": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "preview": "NODE_ENV=production node dist/server",
    "test": "NODE_ENV=test dotenv -e .env.test -- vitest",
    "migrate:test": "dotenv -e .env.test -- npx prisma migrate deploy"
  },
  "dependencies": {
    "@prisma/client": "^5.10.2",
    "@tanstack/react-query": "^5.24.2",
    "@types/express-session": "^1.18.0",
    "axios": "^1.6.7",
    "bcrypt": "^5.1.1",
    "compression": "^1.7.4",
    "connect-redis": "^7.1.1",
    "cookie-parser": "^1.4.6",
    "dotenv": "^16.4.5",
    "dotenv-cli": "^7.4.0",
    "express": "^4.18.3",
    "express-session": "^1.18.0",
    "jsonwebtoken": "^9.0.2",
    "morgan": "^1.10.0",
    "prisma": "^5.10.2",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-helmet": "^6.1.0",
    "react-helmet-async": "^2.0.4",
    "react-router-dom": "^6.22.2",
    "redis": "^4.6.13",
    "sirv": "^2.0.4"
  },
  "devDependencies": {
    "@types/bcrypt": "^5.0.2",
    "@types/compression": "^1.7.5",
    "@types/cookie-parser": "^1.4.7",
    "@types/express": "^4.17.21",
    "@types/jsonwebtoken": "^9.0.6",
    "@types/morgan": "^1.9.9",
    "@types/node": "^20.11.24",
    "@types/react": "^18.2.56",
    "@types/react-dom": "^18.2.19",
    "@types/react-helmet": "^6.1.11",
    "@types/supertest": "^6.0.2",
    "@typescript-eslint/eslint-plugin": "^7.0.2",
    "@typescript-eslint/parser": "^7.0.2",
    "@vitejs/plugin-react": "^4.2.1",
    "eslint": "^8.56.0",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.4.5",
    "nodemon": "^3.1.0",
    "npm-run-all": "^4.1.5",
    "supertest": "^6.3.4",
    "tsconfig-paths": "^4.2.0",
    "tslib": "^2.6.2",
    "tsx": "^4.7.1",
    "typescript": "^5.2.2",
    "vite": "^5.1.4",
    "vite-tsconfig-paths": "^4.3.1",
    "vitest": "^1.3.1",
    "vitest-mock-extended": "^1.3.1"
  }
}
